{
  "manifest_version": 3,
  "name": "Teste de interação com uma spa",
  "version": "1.0",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "host_permissions": [
    "http://localhost:4200/*"
  ],
  "permissions": [
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": ["http://localhost:4200/*"],
      
      "js": ["scripts/content.js"]
    }
  ]
}